{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Fairspec Datacite Profile",
  "allOf": [{ "$ref": "{dataset-ref}" }, { "$ref": "#/$defs/Dataset" }],
  "$defs": {
    "Dataset": {
      "type": "object",
      "required": [
        "$schema",
        "doi",
        "creators",
        "titles",
        "publisher",
        "publicationYear",
        "resourceType"
      ]
    }
  }
}
