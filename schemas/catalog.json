{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["loc", "upd"],
    "additionalProperties": false,
    "properties": {
      "loc": {
        "type": "string",
        "format": "uri"
      },
      "upd": {
        "type": "string",
        "format": "date-time"
      }
    }
  }
}
